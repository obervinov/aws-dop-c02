{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "mccutchen/go-httpbin:latest",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "8080"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/current/logs",
      "ContainerDirectory": "/app/logs"
    }
  ],
  "Logging": "/var/log/nginx"
}